    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python3",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "python": "/usr/bin/python3",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "env": {"GST_DEBUG": "*:3"},
            "envFile": "${workspaceFolder}/.env",
            "debugOptions": [
                "RedirectOutput"
            ],
            "justMyCode": false,
            "subProcess": true,

        }
    ]
}